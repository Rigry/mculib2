// Поместите параметры в этот файл, чтобы перезаписать параметры по умолчанию и пользовательские параметры.
{
    "files.associations": {
        "stm32f4xx.h": "c",
        "stm32f4_bf.h": "c",
        "type_traits": "cpp",
        "initializer_list": "cpp",
        "cstdint": "cpp"
    },
	// Compiler options for C (e.g. ["-std=c99"])
	"clang.cflags": [
        "-I./inc/",
		"-I./inc/CMSIS/",
		"-I./src/",
		"-I./src/usrlib/",
        "-I/usr/lib/arm-none-eabi/include/",
        "-I.",
        "-I./hal",
        "-I./hal/ral",
        "-I./loki",
        "-I./STMF4_files",
        "-I./STMF4_files/CMSIS",
        "-I./test_F4"
	],
    // Показывать значения переменных в редакторе во время отладки
    "debug.inlineValues": true,
    
    "C_Cpp.intelliSenseEngine": "Default",
    "editor.fontSize": 14,
    "editor.tabSize": 3,
    "editor.renderWhitespace": "boundary"
}